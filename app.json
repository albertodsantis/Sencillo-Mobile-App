{
  "expo": {
    "name": "Sencillo",
    "slug": "sencillo",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/IconKitchen-Output/ios/AppIcon~ios-marketing.png",
    "scheme": "sencillo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/IconKitchen-Output/web/icon-512.png",
      "resizeMode": "contain",
      "backgroundColor": "#020617"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sencillo.app"
    },
    "android": {
      "package": "com.sencillo.app",
      "adaptiveIcon": {
        "backgroundColor": "#2d3446",
        "foregroundImage": "./assets/images/IconKitchen-Output/android/res/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "backgroundImage": "./assets/images/IconKitchen-Output/android/res/mipmap-xxxhdpi/ic_launcher_background.png",
        "monochromeImage": "./assets/images/IconKitchen-Output/android/res/mipmap-xxxhdpi/ic_launcher_monochrome.png"
      }
    },
    "web": {
      "favicon": "./assets/images/IconKitchen-Output/web/favicon.ico"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
